local Lighting = game:GetService("Lighting")
local sky = Instance.new("Sky")
sky.Name = "SKIDhack"

sky.SkyboxBk = "rbxassetid://]]..skyId..[["
sky.SkyboxFt = "rbxassetid://]]..skyId..[["
sky.SkyboxLf = "rbxassetid://]]..skyId..[["
