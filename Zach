    btn.Position = UDim2.new(0, posX, 0, posY)
    btn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    btn.BackgroundTransparency = 0.1
    btn.TextScaled = true
    btn.FontFace = Font.new("rbxasset://fonts/families/ComicNeueAngular.json")

    local corner = Instance.new("UICorner", btn)
    return btn
end

local function createTB(name, placeholder, posX, posY, sizeX, sizeY, default)
    local tb = Instance.new("TextBox", LMG2L["ScrollingFrame_a"])
    tb.Name = name
    tb.PlaceholderText = placeholder
    tb.Text = default or ""
    tb.Size = UDim2.new(0, sizeX, 0, sizeY)
    tb.Position = UDim2.new(0, posX, 0, posY)
    tb.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    tb.BackgroundTransparency = 0.1
    tb.TextScaled = true
    tb.FontFace = Font.new("rbxasset://fonts/families/ComicNeueAngular.json")
    tb.ClearTextOnFocus = false

    local corner = Instance.new("UICorner", tb)
    return tb
end

